{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demo-adf21"
		},
		"AdvWorksDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksDB'"
		},
		"AdvWorksDb1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksDb1'"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"ProductBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ProductBlobStorage1'"
		},
		"AdvWorksDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "demosqldbserver21.database.windows.net"
		},
		"AdvWorksDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2019"
		},
		"AdvWorksDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Shivani"
		},
		"AdvWorksDb1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "demosqldbserver21.database.windows.net"
		},
		"AdvWorksDb1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksLT2019"
		},
		"AdvWorksDb1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Shivani"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "demosqldbserver21.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2019"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Shivani"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "demo-adf21_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdvWorksDB_password": {
						"value": "[parameters('AdvWorksDB_password')]"
					},
					"AdvWorksDb1_password": {
						"value": "[parameters('AdvWorksDb1_password')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"ProductBlobStorage1_connectionString": {
						"value": "[parameters('ProductBlobStorage1_connectionString')]"
					},
					"AdvWorksDB_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksDB_properties_typeProperties_server')]"
					},
					"AdvWorksDB_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksDB_properties_typeProperties_database')]"
					},
					"AdvWorksDB_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksDB_properties_typeProperties_userName')]"
					},
					"AdvWorksDb1_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksDb1_properties_typeProperties_server')]"
					},
					"AdvWorksDb1_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksDb1_properties_typeProperties_database')]"
					},
					"AdvWorksDb1_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksDb1_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}